@model IEnumerable<string>

<a asp-controller="Home" asp-action="ViewCrashes">Home</a>

@foreach (string s in Model)
{
    <a asp-controller="Home" asp-action="ViewCrashes" asp-route-crashSeverity="@s">@s</a>
}